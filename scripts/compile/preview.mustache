<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>{{ title }} - {{ file_name }}</title>
        <style>
            body{
                text-align: center;
            }
            canvas{
                border: 2px solid #000;
            }
        </style>
    </head>
    <body>
        <h3> {{ file_name }} </h3>
        <canvas id="c" width="1000" height="500"></canvas>

        <script>module = {}</script>
        {{#modules}}
        <script src="../node_modules/{{{.}}}"></script>
        {{/modules}}

        {{#files}}
        <script src="../{{{.}}}"></script>
        {{/files}}

        <script src="main.js"></script>
    </body>
</html>